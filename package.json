{
  "name": "hede.io",
  "version": "1.0.0",
  "engines": {
    "node": ">=7.10.1",
    "npm": "=5.3.0"
  },
  "bin": "./server",
  "main": "./server",
  "private": true,
  "scripts": {
    "dev-server": "run-p dev-server:auth-server dev-server:client --max-old-space-size=4096",
    "dev-server:client": "webpack-dev-server --watch-poll --config ./webpack/webpack-dev-server.js --inline --hot",
    "dev-server:auth-server": "cross-env NODE_ENV=development HEDE_API=http://localhost:4040/api/ node ./server/auth-server.js",
    "dev-server:server": "cross-env NODE_ENV=development webpack -p --config ./webpack/webpack.server.config.js",
    "start": "cross-env NODE_ENV=production npm run run:server",
    "dev": "cross-env NODE_ENV=development run-p build:server:dev run:server",
    "test": "cross-env STEEMCONNECT_IMG_HOST=https://img.busy.org jest",
    "test:watch": "cross-env STEEMCONNECT_IMG_HOST=https://img.busy.org jest --watch",
    "test:coverage": "cross-env STEEMCONNECT_IMG_HOST=https://img.busy.org jest --coverage",
    "eslint-autofix": "eslint --fix \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\"",
    "run:server": "nodemon --watch hede.server.js hede.server.js",
    "build:server:dev": "cross-env NODE_ENV=development webpack --progress --config ./webpack/webpack.server.config.js",
    "build:server": "cross-env NODE_ENV=production webpack -p --config ./webpack/webpack.server.config.js",
    "build:client": "npm run cleanup-public && mkdir -p public && cp -r ./assets/* ./public/ && webpack --config ./webpack/webpack.prod.config.js",
    "build": "cross-env NODE_ENV=production npm run build:client && npm run build:server",
    "heroku-prebuild": "npm i -g pm2 && pm2 install pm2-logrotate",
    "heroku-postbuild": "npm run build",
    "cleanup-public": "gulp cleanup-public",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "pre-commit": [
    "test",
    "lint"
  ],
  "dependencies": {
    "antd": "^2.13.11",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
